<h2>Edit My Account Information</h2>

<%= form_for(:operator, url: admin_operator_path(current_operator), method: :put) do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :name %>
  <%= f.text_field :name %>


  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Update" %>
<% end %>
