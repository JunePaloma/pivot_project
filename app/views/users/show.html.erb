    <div class="container">
      <div class="row">
        <div class="col s12 m6 offset-m2 dashboard-card">
          <div class="card blue-grey lighten-1">
            <div class="card-content white-text">
                <span class="card-title"> Welcome <%= current_user.username %>  </span>
                <p> <%= current_user.address %> </p>
                <p> <%= current_user.phone %> </p>
                <p> <%= current_user.email %>  </p>
            </div>
            <div class="card-action">
              <%= link_to "Edit My Account", edit_user_path(current_user), class: "black-text" %>
              <%= link_to "View Store Requests", store_requests_path, class: "black-text" %>
            </div>
          </div>
        </div>
      </div>
    </div>
